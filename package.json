{
  "name": "tianxuan-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "一个专为90后、00后年轻人打造的玄学决策助手网页应用",
  "author": "TianXuan Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tianxuan-web.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "tailwindcss",
    "易经",
    "占卜",
    "塔罗牌",
    "玄学",
    "决策助手"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "check": "npm outdated",
    "lint": "eslint src --ext .js,.vue,.ts",
    "clean:deps": "rm -rf node_modules && npm install",
    "git:status": "git status",
    "git:add": "git add .",
    "git:commit": "git commit -m",
    "git:push": "git push origin",
    "git:pull": "git pull origin",
    "git:log": "git log --oneline -10",
    "git:check": ".\\scripts\\check-git-status.bat",
    "git:init": ".\\scripts\\execute-git-control.bat",
    "git:simple": ".\\scripts\\simple-git-init.bat",
    "git:ps": "powershell -ExecutionPolicy Bypass -File .\\scripts\\git-init.ps1",
    "git:commit-msg": ".\\scripts\\git-commit.bat",
    "git:release": ".\\scripts\\create-release.bat"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@supabase/supabase-js": "^2.51.0",
    "@types/html2canvas": "^0.5.35",
    "@types/three": "^0.178.1",
    "@types/uuid": "^10.0.0",
    "@vueuse/core": "^10.11.1",
    "animejs": "^3.2.1",
    "axios": "^1.4.0",
    "html2canvas": "^1.4.1",
    "lottie-web": "^5.12.2",
    "marked": "^16.0.0",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "three": "^0.178.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.7",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.5.14",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
